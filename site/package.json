{
  "name": "next-commerce",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "analyze": "BUNDLE_ANALYZE=both next build",
    "lint": "next lint",
    "env": "gen-env-types .env --example-env-path . --types-output src/@types/env.d.ts",
    "prettier-fix": "prettier --write .",
    "find:unused": "npx next-unused"
  },
  "sideEffects": false,
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.4",
    "@prisma/client": "^4.2.1",
    "@radix-ui/colors": "^0.1.8",
    "@radix-ui/react-dropdown-menu": "^1.0.0",
    "@radix-ui/react-icons": "^1.1.1",
    "@radix-ui/react-scroll-area": "^1.0.0",
    "@radix-ui/react-select": "^1.0.0",
    "@radix-ui/react-switch": "^1.0.0",
    "@stitches/react": "^1.2.8",
    "@supabase/supabase-js": "^1.35.6",
    "@trpc/client": "^9.27.1",
    "@trpc/next": "^9.27.1",
    "@trpc/react": "^9.27.1",
    "@trpc/server": "^9.27.1",
    "fuse.js": "^6.6.2",
    "next": "^12.0.8",
    "next-auth": "^4.10.3",
    "next-images": "^1.8.4",
    "next-themes": "^0.0.15",
    "phosphor-react": "^1.4.1",
    "prisma": "^4.2.1",
    "react": "^18.2.0",
    "react-country-flag": "^3.0.2",
    "react-dom": "^18.2.0",
    "react-query": "3",
    "react-simple-star-rating": "^4.0.5",
    "react-typical": "^0.1.3",
    "sass": "^1.54.4",
    "stripe": "^10.3.0",
    "swiper": "^8.3.2",
    "zod": "^3.18.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.0.8",
    "@types/node": "^17.0.8",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "eslint": "^8.6.0",
    "eslint-config-next": "^12.0.8",
    "eslint-config-prettier": "^8.3.0",
    "gen-env-types": "^1.3.4",
    "lint-staged": "^12.1.7",
    "prettier": "^2.5.1",
    "typescript": "4.3.4"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint",
      "prettier --write",
      "git add"
    ],
    "**/*.{md,mdx,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "next-unused": {
    "alias": {
      "@lib/*": [
        "lib/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@config/*": [
        "config/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@utils/*": [
        "utils/*"
      ]
    },
    "debug": true,
    "include": [
      "components",
      "lib",
      "pages"
    ],
    "exclude": [],
    "entrypoints": [
      "pages"
    ]
  }
}
